#include <stdio.h>
#include <stdlib.h>

int main(void) {
	setvbuf(stdout, NULL, _IONBF, 0);
	double d = 3.1416;
	char ch = *(char *) &d;
	printf("%c", ch);
	return EXIT_SUCCESS;
}
